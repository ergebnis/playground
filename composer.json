{
  "name": "ergebnis/playground",
  "description": "Provides a GitHub repository template for a PHP library.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/playground",
  "support": {
    "issues": "https://github.com/ergebnis/playground/issues",
    "source": "https://github.com/ergebnis/playground"
  },
  "require": {
    "php": "~8.1.0 || ~8.2.0 || ~8.3.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.42.0",
    "ergebnis/license": "^2.4.0",
    "ergebnis/php-cs-fixer-config": "~6.20.0",
    "ergebnis/phpunit-slow-test-detector": "^2.9.0",
    "fakerphp/faker": "^1.23.1",
    "infection/infection": "~0.27.9",
    "phpunit/phpunit": "^10.5.9",
    "psalm/plugin-phpunit": "~0.18.4",
    "rector/rector": "~0.18.13",
    "roave/backward-compatibility-check": "^8.6.0",
    "vimeo/psalm": "^5.21"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\Playground\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\Playground\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true
    },
    "audit": {
      "abandoned": "report"
    },
    "platform": {
      "php": "8.1.26"
    },
    "preferred-install": "dist",
    "sort-packages": true
  }
}
